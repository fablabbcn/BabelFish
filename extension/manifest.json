{
    "name": "chrome-rpc",
    "version": "0.1",
    "manifest_version": 2,
    "description": "RPC protocol.",
    "app": {
	"background": {
	    "scripts": [ "config.js", "rpc-host.js", "background.js" ], // Make config remote
	    "persistent": true
	}
    },
    "externally_connectable": {
	"matches": [
	    "http://localhost:8080/*"
	],
	"accepts_tls_channel_id": false
    },
    "permissions": [
	"serial",
	"http://localhost:8080/*"
    ]
}
