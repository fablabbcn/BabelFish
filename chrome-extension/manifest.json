{
    "name": "babel-fish",
    "version": "0.0.0.1",
    "manifest_version": 2,
    "description": "Serial for codebender.",
    "app": {
	"background": {
	    "page": "background.html"
	}
    },
    "externally_connectable": {
	"matches": [
            "http://localhost:8080/*",
            "http://staging.codebender.cc/*",
            "https://staging.codebender.cc/*",
            "http://codebender.cc/*",
            "https://codebender.cc/*"
	],
	"accepts_tls_channel_id": false
    },
    "permissions": [
        "storage",
	"serial",
        "usb",
        "http://localhost:8080/*",
        "http://localhost/*",
        "http://staging.codebender.cc/*",
        "https://staging.codebender.cc/*",
        "http://codebender.cc/*",
        "https://codebender.cc/*"
    ]
}
