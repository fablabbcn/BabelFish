{
    "name": "babel-fish",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Serial for everyone.",
    "app": {
	"background": {
	    "page": "background.html"
	}
    },
    "externally_connectable": {
	"matches": [
	    "http://localhost:8080/*"
	],
	"accepts_tls_channel_id": false
    },
    "permissions": [
	"storage",
	"alarms",
	"notifications",
        { "fileSystem": ["write"]},
	"syncFileSystem",
	"serial",
	"http://localhost:8080/*"
    ]
}
