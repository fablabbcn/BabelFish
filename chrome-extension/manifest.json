{
    "name": "Codebender app",
    "version": "0.1.0.10",
    "manifest_version": 2,
    "minimum_chrome_version": "41",
    "author": "Codebender.cc",
    "icons": {"16": "logo16.png",
              "48": "logo48.png",
              "128": "logo128.png"},
    "description": "Serial for codebender and the rest of the world.",
    "app": {
	"background": {
	    "page": "background.html"
	}
    },
    "externally_connectable": {
	"matches": [
            "http://localhost:8080/*",
            "http://*.codebender.cc/*",
            "https://*.codebender.cc/*",
            "http://codebender.cc/*",
            "https://codebender.cc/*"
	],
	"accepts_tls_channel_id": false
    },
    "permissions": [
        {
            "usbDevices": [
                {"vendorId": 6017, "productId": 3231}
            ]
        },
        "storage",
	"serial",
        "usb",
        "http://localhost:8080/*",
        "http://localhost/*",
        "http://*.codebender.cc/*",
        "https://*.codebender.cc/*",
        "http://codebender.cc/*",
        "https://codebender.cc/*"
    ]
}
