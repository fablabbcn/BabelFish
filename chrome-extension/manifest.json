{
"update_url": "https://clients2.google.com/service/update2/crx",
    "name": "Smart Citizen Kit",
    "version": "0.1.1.2",
    "manifest_version": 2,
    "minimum_chrome_version": "41",
    "author": "Smart Citizen",
    "icons": {"16": "logo16.png",
              "48": "logo48.png",
              "128": "logo128.png"},
    "description": "Smart Citizen Kit configuration tool",
    "app": {
	"background": {
	    "page": "background.html"
	}
    },
    "externally_connectable": {
	"matches": [
            "http://localhost:8080/*",
            "http://localhost/*",
            "https://smartcitizen.me/*",
            "http://smartcitizen.me/*",
            "https://sandbox.smartcitizen.me/*",
            "http://sandbox.smartcitizen.me/*"
	],
	"accepts_tls_channel_id": false
    },
    "permissions": [
        "storage",
    	"serial",
        "usb",
        "http://localhost:8080/*",
        "http://localhost/*",
        "https://smartcitizen.me/*",
        "http://smartcitizen.me/*",
        "https://sandbox.smartcitizen.me/*",
        "http://sandbox.smartcitizen.me/*"
    ]
}
