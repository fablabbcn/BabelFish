{
    "name": "babel-fish",
    "version": "0.1.0.9",
    "manifest_version": 2,
    "minimum_chrome_version": "38",
    "author": "Codebender.cc",
    "icons": {"16": "logo16.png",
              "48": "logo48.png",
              "128": "logo128.png"},
    "description": "Serial for codebender and the rest of the world.",
    "app": {
	"background": {
	    "page": "background.html"
	}
    },
    "externally_connectable": {
	"matches": [
            "http://localhost:8080/*",
            "http://localhost/*",
            "http://staging.codebender.cc/*",
            "https://staging.codebender.cc/*",
            "http://codebender.cc/*",
            "https://codebender.cc/*"
	],
	"accepts_tls_channel_id": false
    },
    "permissions": [
        "storage",
	"serial",
        "usb",
        "http://localhost:8080/*",
        "http://localhost/*",
        "http://staging.codebender.cc/*",
        "https://staging.codebender.cc/*",
        "http://codebender.cc/*",
        "https://codebender.cc/*"
    ]
}
