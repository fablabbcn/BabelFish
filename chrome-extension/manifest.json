{
    "name": "babel-fish",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Serial for everyone.",
    "app": {
	"background": {
	    "page": "background.html"
	}
    },
    "externally_connectable": {
	"matches": [
	    "http://ec2-54-174-134-98.compute-1.amazonaws.com:8080/*",
        "http://localhost:8080/*",
        "http://localhost/*",
        "http://staging.codebender.cc/*",
        "https://staging.codebender.cc/*"
	],
	"accepts_tls_channel_id": false
    },
    "permissions": [
        {"usbDevices": [
            {
                "vendorId": 2652,
                "productId": 17664
            }, {
                "vendorId": 33417,
                "productId": 17664
            }, {
                "vendorId": 6017,
                "productId": 3231
            }
        ]},
        "storage",
	"alarms",
	"notifications",
        { "fileSystem": ["write"]},
	"syncFileSystem",
	"serial",
    "usb",
    "http://localhost:8080/*",
	"http://ec2-54-174-134-98.compute-1.amazonaws.com:8080/*",
    "http://localhost/*",
    "http://staging.codebender.cc/*",
    "https://staging.codebender.cc/*"
    ]
}
